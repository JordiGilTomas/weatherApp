{
  "name": "weatherapp",
  "version": "1.0.0",
  "description": "A Weather APP",
  "private": true,
  "type": "module",
  "browserslist": ["> 0.5% or last 2 versions, not dead"],
  "scripts": {
    "start": "node --experimental-modules src/index.js",
    "ui": "browser-sync start --proxy=localhost:8000 --files='**/*.css, **/*.js' --ignore=node_modules --reload-delay 10 --no-ui --no-notify--reload-delay 10",
    "dev": "start npm run ui && start nodemon src/index.js",
    "webp": "webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JordiGilTomas/weatherApp.git"
  },
  "keywords": [
    "weather"
  ],
  "author": "Jordi Gil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JordiGilTomas/weatherApp/issues"
  },
  "homepage": "https://github.com/JordiGilTomas/weatherApp#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "handlebars": "^4.7.6",
    "minimist": "^1.2.5",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "browser-sync": "^2.26.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "handlebars-loader": "^1.7.1",
    "mkdirp": "^1.0.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
